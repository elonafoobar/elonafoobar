locale {
    item {
        bug {
            name = "黄金"
        }
        long_sword {
            name = "長剣"
            katakana_name = "ロングソード"
            description {
                main {
                    text = "細い刀身の剣だ。"
                    footnote = "〜鑑定報告書：＜武器＞カテゴリ〜"
                }
                _0 {
                    text = "長い刀身を携えた、斬ることを広く目的とした剣。その他種多様さは今も尚進化を続けているという。"
                    footnote = "〜明日から使えるあなたの為の武具集〜"
                }
            }
        }
        dagger {
            name = "短剣"
            katakana_name = "ダガー"
            description {
                main {
                    text = "小型の剣だ。"
                    footnote = "〜鑑定報告書：＜武器＞カテゴリ〜"
                }
                _0 {
                    text = "古代の氏族が発端の、片手でも簡単に扱えるよう短く軽い小剣。そのシンプルな形状は現在でも変わることなく存り続けている。"
                    footnote = "〜明日から使えるあなたの為の武具集〜"
                }
            }
        }
        hand_axe {
            name = "手斧"
            katakana_name = "ハンドアクス"
            description {
                main {
                    text = "小型の斧だ。"
                    footnote = "〜鑑定報告書：＜武器＞カテゴリ〜"
                }
                _0 {
                    text = "片手で扱えるよう軽く作られている斧。武器というより薪を割る等日常的な側面が色濃く残る作りをしているが、その軽さを生かし棍棒の様に使える万能さも持っている。"
                    footnote = "〜明日から使えるあなたの為の武具集〜"
                }
            }
        }
        club {
            name = "棍棒"
            katakana_name = "クラブ"
            description {
                main {
                    text = "素材を削り出して作られた棒だ。"
                    footnote = "〜鑑定報告書：＜武器＞カテゴリ〜"
                }
                _0 {
                    text = "殴打する為に作られた簡素な武器。作成方法も使い方も非常にシンプルな為、冒険者にも愛用者が多いという。"
                    footnote = "〜明日から使えるあなたの為の武具集〜"
                }
            }
        }
        magic_hat {
            name = "魔法帽"
            katakana_name = "マジックハット"
            description {
                main {
                    text = "魔法使いのための帽子だ。"
                    footnote = "〜鑑定報告書：＜防具＞カテゴリ〜"
                }
                _0 {
                    text = "魔道士が身につけていそうな三角形の帽子。特に得られる効果はないが、被ることで何となく賢くなった気分にはさせてくれる。"
                    footnote = "〜明日から使えるあなたの為の武具集〜"
                }
            }
        }
        fairy_hat {
            name = "フェアリーハット"
            katakana_name = "フェアリーハット"
            description {
                main {
                    text = "妖精向けの帽子だ。"
                    footnote = "〜鑑定報告書：＜防具＞カテゴリ〜"
                }
                _0 {
                    text = "妖精達が身につけるという非常に軽い帽子。か弱き存在と自負する故か、その帽子には外界の変異から身を守る能力が備わっているという。"
                    footnote = "〜明日から使えるあなたの為の武具集〜"
                }
            }
        }
        breastplate {
            name = "鎧"
            katakana_name = "メイル"
            description {
                main {
                    text = "身を守る為に作られた戦闘用の防具だ。"
                    footnote = "〜鑑定報告書：＜防具＞カテゴリ〜"
                }
                _0 {
                    text = "武器の進化に合わせて開発された防具。重量はあるが急所に致命傷を与えない役目は十分に果たしている。"
                    footnote = "〜明日から使えるあなたの為の武具集〜"
                }
            }
        }
        robe {
            name = "法衣"
            katakana_name = "ローブ"
            description {
                main {
                    text = "僧侶用の服だ。"
                    footnote = "〜鑑定報告書：＜防具＞カテゴリ〜"
                }
                _0 {
                    text = "主に僧侶などが着用する、一枚の布に素材を編み込んだ防具。その製法故身を守るには適さないが、装着者の詠唱を妨害せず、身軽な行動を可能とさせている。"
                    footnote = "〜明日から使えるあなたの為の武具集〜"
                }
            }
        }
        decorated_gloves {
            name = "細工篭手"
            katakana_name = "シニアグローブ"
            description {
                main {
                    text = "飾りのついた篭手だ。"
                    footnote = "〜鑑定報告書：＜防具＞カテゴリ〜"
                }
                _0 {
                    text = "様々な細工が散りばめられた上等な篭手。儀礼的な要素が強いが、それでも装飾によっていくらかの防護にはなる。"
                    footnote = "〜明日から使えるあなたの為の武具集〜"
                }
            }
        }
        thick_gauntlets {
            name = "厚篭手"
            katakana_name = "ライトガントレット"
            description {
                main {
                    text = "分厚い篭手だ。"
                    footnote = "〜鑑定報告書：＜防具＞カテゴリ〜"
                }
                _0 {
                    text = "手首から先を守る為に作られた防具。手先の自由は少々奪われるが手を失うよりはましだろう。"
                    footnote = "〜明日から使えるあなたの為の武具集〜"
                }
            }
        }
        heavy_boots {
            name = "重靴"
            katakana_name = "ヘビーブーツ"
            description {
                main {
                    text = "重い靴だ。"
                    footnote = "〜鑑定報告書：＜防具＞カテゴリ〜"
                }
                _0 {
                    text = "沢山の素材片を張り付けた靴。当然通常より重くなっているが、その分防護は硬くなっている。"
                    footnote = "〜明日から使えるあなたの為の武具集〜"
                }
            }
        }
        composite_boots {
            name = "合成靴"
            katakana_name = "コンポジットブーツ"
            description {
                main {
                    text = "固い靴だ。"
                    footnote = "〜鑑定報告書：＜防具＞カテゴリ〜"
                }
                _0 {
                    text = "特殊な素材をかけ合わせてより強固な防護を得た靴。非常に硬く、歩くだけで心地よい音が辺りに響くと言う。"
                    footnote = "〜明日から使えるあなたの為の武具集〜"
                }
            }
        }
        decorative_ring {
            name = "装飾の指輪"
            katakana_name = "デコレートリング"
            description {
                main {
                    text = "幾つかの装飾がされた指輪だ。"
                    footnote = "〜鑑定報告書：＜防具＞カテゴリ〜"
                }
                _0 {
                    text = "様々な飾りがとても美しい指輪。かつて、この小さな円の中にどれだけの彩を凝らすかで職人の腕が競われたことがあったという。"
                    footnote = "〜明日から使えるあなたの為の武具集〜"
                }
            }
        }
        scroll_of_identify {
            name = "鑑定の巻物"
            description {
                main {
                    text = "所持アイテムの鑑定を行う巻物だ。"
                    footnote = "〜鑑定報告書：＜紙片＞カテゴリ〜"
                }
                _0 {
                    text = "未鑑定の品を鑑定する巻物。強力な品の場合鑑定できない場合があるが、そういった場合は魔道士の手を借りよう。"
                    footnote = "〜魔具全典〜"
                }
            }
        }
        scroll_of_oracle {
            name = "神託の巻物"
            description {
                main {
                    text = "今までに現れた★のつくアイテムの現在地を知ることができる巻物だ。"
                    footnote = "〜鑑定報告書：＜紙片＞カテゴリ〜"
                }
                _0 {
                    text = "伝説の品の存在を知ることができるとされる巻物。誰が持っているかまでは教えてくれない、神はそこまであなたに親身ではないのだ。"
                    footnote = "〜魔具全典〜"
                }
            }
        }
        scroll_of_teleportation {
            name = "テレポートの巻物"
            description {
                main {
                    text = "ランダムな位置へテレポートを行う巻物だ。"
                    footnote = "〜鑑定報告書：＜紙片＞カテゴリ〜"
                }
                _0 {
                    text = "空間に歪みを作り出すことで一瞬のうちに別の場所に移動することができる巻物。緊急時には便利だが行き先までは指定できないので待ち合わせに遅れた時等には使えないだろう。"
                    footnote = "〜魔具全典〜"
                }
            }
        }
        scroll_of_incognito {
            name = "インコグニートの巻物"
            description {
                main {
                    text = "モンスター以外の敵対状況をリセットする巻物だ。"
                    footnote = "〜鑑定報告書：＜紙片＞カテゴリ〜"
                }
                _0 {
                    text = "一瞬で別人になりすますことができる巻物。使った時の気分はさながら国を動かす大怪盗だ。"
                    footnote = "〜魔具全典〜"
                }
            }
        }
        rod_of_identify {
            name = "鑑定の杖"
            description {
                main {
                    text = "振ることで所持アイテムの鑑定を行う魔法の杖だ。"
                    footnote = "〜鑑定報告書：＜魔道具＞カテゴリ〜"
                }
                _0 {
                    text = "特定の魔法が封じ込められた杖。全てを見透かすような透明な宝石が取り付けられている。"
                    footnote = "〜魔具全典〜"
                }
            }
        }
        rod_of_teleportation {
            name = "テレポートの杖"
            description {
                main {
                    text = "振ることでランダムなテレポートを行う魔法の杖だ。"
                    footnote = "〜鑑定報告書：＜魔道具＞カテゴリ〜"
                }
                _0 {
                    text = "特定の魔法が封じ込められた杖。四角い透き通った緑色の宝石が取り付けられている。"
                    footnote = "〜魔具全典〜"
                }
            }
        }
        spellbook_of_teleportation {
            name = "テレポートの魔法書"
            description {
                main {
                    text = "テレポートを唱える為に必要な本。読むことができる。"
                    footnote = "〜鑑定報告書：＜書物＞カテゴリ〜"
                }
                _0 {
                    text = "「テレポート」という呪文について学ぶことができる魔法書。今すぐ旅に出たいあなたに。"
                    footnote = "〜本の為の本・魔法書編〜"
                }
            }
        }
        spellbook_of_identify {
            name = "鑑定の魔法書"
            description {
                main {
                    text = "アイテムを鑑定する呪文を唱える為に必要な本。読むことができる。"
                    footnote = "〜鑑定報告書：＜書物＞カテゴリ〜"
                }
                _0 {
                    text = "「鑑定」という呪文について学ぶことができる魔法書。何でも疑う癖のあるあなたに。"
                    footnote = "〜本の為の本・魔法書編〜"
                }
            }
        }
        spellbook_of_uncurse {
            name = "解呪の魔法書"
            description {
                main {
                    text = "アイテムの呪いを解く呪文を唱える為に必要な本。読むことができる。"
                    footnote = "〜鑑定報告書：＜書物＞カテゴリ〜"
                }
                _0 {
                    text = "「解呪」という呪文について学ぶことができる魔法書。霊の存在を信じるあなたに。"
                    footnote = "〜本の為の本・魔法書編〜"
                }
            }
        }
        book_a {
            name = "本"
            unidentified_name = "本"
            description {
                main {
                    text = "雑多な情報が書かれた本。特に利用できるものはない。"
                    footnote = "〜鑑定報告書：＜書類＞カテゴリ〜"
                }
                _0 {
                    text = "ノースティリスで広く流通している記録媒体。中には重要なことが書かれているものがあるが、その殆どがとるに値しない端書程度の内容だ。"
                    footnote = "〜本の為の本〜"
                }
            }
        }
        book_b {
            name = "本"
            unidentified_name = "本"
            description {
                main {
                    text = "雑多な情報が書かれた本。特に利用できるものはない。"
                    footnote = "〜鑑定報告書：＜書類＞カテゴリ〜"
                }
                _0 {
                    text = "ノースティリスで広く流通している記録媒体。中には重要なことが書かれているものがあるが、その殆どがとるに値しない端書程度の内容だ。"
                    footnote = "〜本の為の本〜"
                }
            }
        }
        bugged_book {
            name = "バグの本"
            unidentified_name = "バグの本"
        }
        bottle_of_dirty_water {
            name = "汚水"
            unidentified_name = "透明なポーション"
            description {
                main {
                    text = "飲むと病気になる可能性のあるポーションだ。"
                    footnote = "〜鑑定報告書：＜液体＞カテゴリ〜"
                }
                _0 {
                    text = "ノースティリスにおいて、市民が普段の生活に使用している水を空き瓶に詰めたもの。生活用水であるものの、現在のノースティリスの水質汚染度は高く、おいそれと飲用するべきではないだろう。"
                    footnote = "〜飲めるのみもの、飲めないのみもの〜"
                }
            }
        }
        potion_of_blindness {
            name = "盲目のポーション"
            description {
                main {
                    text = "盲目状態を引き起こすポーションだ。"
                    footnote = "〜鑑定報告書：＜液体＞カテゴリ〜"
                }
                _0 {
                    text = "身がとても辛いことで有名な火蟹のエキスを余す所なく用いた水薬。飲めば忽ち汗が吹き出し目に入り、戦闘どころではなくなってしまう。その発汗作用により痩せる効果があると勘違いされ、しばしば売れるらしい。"
                    footnote = "〜飲めるのみもの、飲めないのみもの〜"
                }
                _2 {
                    text = "パンプキン達のエキスを抽出し、透明薬を作り出す研究中に生まれた失敗作。飲むことで透明化することを目的としていたが、モンスターのエキスは真っ先に目に反応し、自分以外を認識できなくさせてしまったのだ。"
                    footnote = "〜飲めるのみもの、飲めないのみもの〜"
                }
            }
        }
        potion_of_confusion {
            name = "混乱のポーション"
            description {
                main {
                    text = "混乱状態を引き起こすポーションだ。"
                    footnote = "〜鑑定報告書：＜液体＞カテゴリ〜"
                }
                _0 {
                    text = "飲むと頭に激痛を引き起こす水薬。頭痛は一過性のものだが、平衡感覚や読解力、会話能力の著しい低下を誘発させる。また酩酊時の症状と似ている為、誤飲した場合泥酔者と間違われるので取り扱いには注意しよう。"
                    footnote = "〜飲めるのみもの、飲めないのみもの〜"
                }
            }
        }
        potion_of_paralysis {
            name = "麻痺のポーション"
            description {
                main {
                    text = "麻痺状態を引き起こすポーションだ。"
                    footnote = "〜鑑定報告書：＜液体＞カテゴリ〜"
                }
                _0 {
                    text = "パラライザーを煎じたものを水薬に混ぜたもの。飲むと肉体に障害をきたすほどの痺れを生じさせる。誤飲してしまっても一過性のものである為心配はないが、癖にならないようにと忠告しておこう。"
                    footnote = "〜飲めるのみもの、飲めないのみもの〜"
                }
            }
        }
        sleeping_drug {
            name = "睡眠薬"
            description {
                main {
                    text = "睡眠状態を引き起こすポーションだ。"
                    footnote = "〜鑑定報告書：＜液体＞カテゴリ〜"
                }
                _0 {
                    text = "甘い香りのする水薬。振りかければどんな猛獣でも忽ち大人しくなるという。不眠症のあなたにも是非とも一本。"
                    footnote = "〜飲めるのみもの、飲めないのみもの〜"
                }
            }
        }
        bottle_of_crim_ale {
            name = "クリムエール"
            unidentified_name = "透明なポーション"
            description {
                main {
                    text = "飲むと酔っぱらう飲み物だ。"
                    footnote = "〜鑑定報告書：＜液体＞カテゴリ〜"
                }
                _0 {
                    text = "ダルフィで密やかに作られる成分不明の酒。ビアと同様痺れる様な刺激はあるが、果実めいた香りが後味を綺麗に整える。尚、この酒は飲み続けていると幻覚を引き起こすとも言われるが詳細は不明。"
                    footnote = "〜奥深い酒の世界〜"
                }
            }
        }
        spellbook_of_ice_bolt {
            name = "アイスボルトの魔法書"
            description {
                main {
                    text = "氷属性のボルトを唱える為に必要な本。読むことができる。"
                    footnote = "〜鑑定報告書：＜書物＞カテゴリ〜"
                }
                _0 {
                    text = "「アイスボルト」という呪文について学ぶことができる魔法書。暑がりなあなたに。"
                    footnote = "〜本の為の本・魔法書編〜"
                }
            }
        }
        spellbook_of_fire_bolt {
            name = "ファイアボルトの魔法書"
            description {
                main {
                    text = "炎属性のボルトを唱える為に必要な本。読むことができる。"
                    footnote = "〜鑑定報告書：＜書物＞カテゴリ〜"
                }
                _0 {
                    text = "「ファイアボルト」という呪文について学ぶことができる魔法書。寒がりなあなたに。"
                    footnote = "〜本の為の本・魔法書編〜"
                }
            }
        }
        spellbook_of_lightning_bolt {
            name = "ライトニングボルトの魔法書"
            description {
                main {
                    text = "雷属性のボルトを唱える為に必要な本。読むことができる。"
                    footnote = "〜鑑定報告書：＜書物＞カテゴリ〜"
                }
                _0 {
                    text = "「ライトニングボルト」という呪文について学ぶことができる魔法書。いつでも雷を見たいあなたに。"
                    footnote = "〜本の為の本・魔法書編〜"
                }
            }
        }
        earth_crystal {
            name = "大地の結晶"
            description {
                main {
                    text = "橙色をした鉱石だ。"
                    footnote = "〜鑑定報告書：＜鉱物＞カテゴリ〜"
                }
                _0 {
                    text = "大地の力を封じ込めたとされる橙色の結晶。光に透かすと、内部にたくさん走ったヒビに反射してとてもきれいだ。"
                    footnote = "〜ヴェルニース鉱物図鑑〜"
                }
            }
        }
        mana_crystal {
            name = "魔力の結晶"
            description {
                main {
                    text = "赤色をした鉱石だ。"
                    footnote = "〜鑑定報告書：＜鉱物＞カテゴリ〜"
                }
                _0 {
                    text = "魔力を封じ込めたとされる赤色の結晶。鉱物自体も純粋な魔力と同様に透き通っている。緊急時には魔道士がこれを砕いてその身に宿したいう話もあるそうだ。"
                    footnote = "〜ヴェルニース鉱物図鑑〜"
                }
                _2 {
                    text = "魔術を操る力を封じ込めたとされる赤色の結晶。他の結晶とは違い人工的に作られていて、かの光り輝く異世界の剣はこの結晶を削り出して力の源にしているという噂もある。"
                    footnote = "〜ヴェルニース鉱物図鑑〜"
                }
            }
        }
        sun_crystal {
            name = "太陽の結晶"
            description {
                main {
                    text = "黄色をした鉱石だ。"
                    footnote = "〜鑑定報告書：＜鉱物＞カテゴリ〜"
                }
                _0 {
