                main {
                    text = "畑においておく鳥よけだ。"
                    footnote = "〜鑑定報告書：＜用具＞カテゴリ〜"
                }
                _0 {
                    text = "両手を大きく広げ害獣を威嚇する、農地の守護者。その足が地面に突き刺さっているのは決してその場から離れることなく命令を従事しようとする意志の現れだろう。"
                    footnote = "〜嘘と勘違いされる全くの作り話・第２巻〜"
                }
            }
        }
        broom {
            name = "ほうき"
            description {
                main {
                    text = "掃除用の道具だ。"
                    footnote = "〜鑑定報告書：＜用具＞カテゴリ〜"
                }
                _0 {
                    text = "掃除をする際に用いる用具。古来魔法使いを象徴するとされ忌み嫌われてきたが、現在は専ら子供達のチャンバラ用の遊具と化している。"
                    footnote = "〜家庭を彩る日用雑貨〜"
                }
            }
        }
        long_pillar {
            name = "長い柱"
            description {
                main {
                    text = "長くて太い柱。それはとても重い。"
                    footnote = "〜鑑定報告書：＜調度品＞カテゴリ〜"
                }
                _0 {
                    text = "天へ真っすぐ伸びた、雄々しく、とても重い柱だ。微動だにしないその姿は、大地の神を思い起こさせる。"
                    footnote = "〜ルミエスト美術目録〜"
                }
            }
        }
        broken_pillar {
            name = "折れた柱"
            description {
                main {
                    text = "折れてしまった柱。それはとても重い。"
                    footnote = "〜鑑定報告書：＜調度品＞カテゴリ〜"
                }
                _0 {
                    text = "何らかの理由により根元から折れてしまった、とても重い柱だ。その独自の形は、どことなく芸術性を感じる。"
                    footnote = "〜ルミエスト美術目録〜"
                }
            }
        }
        street_lamp {
            name = "街灯"
            description {
                main {
                    text = "備え付けられた灯。夜間、周囲を明るく照らす。"
                    footnote = "〜鑑定報告書：＜建造物＞カテゴリ〜"
                }
                _0 {
                    text = "それは第二の月明かり。彼らはいつでも休むことなく道行く人々に影を作っているのだ。"
                    footnote = "〜街中の名脇役達〜"
                }
            }
        }
        water_tub {
            name = "水桶"
