                }
                _0 {
                    text = "小麦を挽いた粉。加工することによって様々なパンが作られる。稀にそのまま食する奇特な人物もいるが、何か故あってのことだろうから、そっとしておいてあげよう。"
                    footnote = "〜移り変わりゆくティリスの食〜"
                }
            }
        }
        bomb_fish {
            name = "はじけ魚"
