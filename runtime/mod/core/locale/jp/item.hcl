                    footnote = "〜鑑定報告書：＜液体＞カテゴリ〜"
                }
                _0 {
                    text = "飲むことにより精神に作用していた悪い影響を平常時まで回復させることのできる水薬。あくまでもいつもの自分に戻るだけなので、格好よかったあの頃に戻る、等といった使い方はできない。"
                    footnote = "〜飲めるのみもの、飲めないのみもの〜"
                }
            }
        }
        potion_of_potential {
            name = "潜在能力のポーション"
